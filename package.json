{
	"name": "ingenium",
	"version": "0.1.0",
	"main": "public/electron.js",
	"author": "Nicocchi",
	"description": "Wallpaper Engine for Linux",
	"homepage": "./",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.9.5",
		"@material-ui/icons": "^4.11.2",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.4.1",
		"@testing-library/user-event": "^7.2.1",
		"cross-env": "^7.0.0",
		"electron-is-dev": "^1.1.0",
		"electron-log": "^4.0.7",
		"electron-store": "^5.1.1",
		"file-base64": "^1.0.0",
		"nanoid": "^3.1.20",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"react-file-base64": "^1.0.3",
		"simple-thumbnail": "^1.6.5",
		"v8-compile-cache": "^2.1.1",
		"video-thumbnail-generator": "^1.1.3"
	},
	"devDependencies": {
		"concurrently": "^5.1.0",
		"electron": "^8.0.2",
		"electron-builder": "^22.3.2",
		"react-app-rewired": "^2.1.5",
		"react-scripts": "^3.4.0",
		"url-loader": "^3.0.0",
		"wait-on": "^4.0.1"
	},
	"scripts": {
		"react-start": "react-scripts start",
		"react-start-rewired": "react-app-rewired start",
		"react-build": "react-scripts build",
		"react-test": "react-scripts test",
		"react-eject": "react-scripts eject",
		"electron-build": "electron-builder",
		"release": "npm run react-build && electron-builder --publish=always",
		"build": "npm run react-build && npm run electron-build",
		"start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron . --allow-file-access-from-files\"",
		"electron-start": "electron . --allow-file-access-from-files",
		"preelectron-pack": "npm run build",
		"electron-pack": "build --em.main=build/electron.js"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"build": {
		"appId": "nonExistentID",
		"files": [
			"build/**/*",
			"node_modules/**/*"
		],
		"directories": {
			"buildResources": "assets"
		}
	}
}
